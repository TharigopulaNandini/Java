import pdfplumber

def filter_rect_metadata(pdf_path, page_number=0):
    with pdfplumber.open(pdf_path) as pdf:
        page = pdf.pages[page_number]
        rects = page.objects.get("rect", [])
        
        # Keep only essential fields
        filtered_rects = [
            {
                "x0": rect["x0"],
                "y0": rect["y0"],
                "x1": rect["x1"],
                "y1": rect["y1"],
                "width": rect["width"]
            }
            for rect in rects
        ]
        return filtered_rects

# Example usage
pdf_path = "example.pdf"
filtered_rects = filter_rect_metadata(pdf_path, page_number=25)

for rect in filtered_rects:
    print(rect)
